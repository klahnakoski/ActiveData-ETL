{
	"threads": 1,
	"batch_size": 5000,
	"sample_only": [],
	"elasticsearch": {
		"host": "http://192.168.2.98",
		"port": 9200,
		"index": "jobs",
		"type": "job",
		"timeout": 300,
		"schema": {
			"$ref": "//../../schema/jobs.json"
		},
		"debug": false,
		"limit_replicas": false
	},
	"source": {
		"$ref": "file://~/private.json#aws_credentials",
		"bucket": "active-data-jobs-beta",
		"key_format": "a.b"
	},
	"work_queue": {
		"name": "active-data-index-jobs-beta",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"constants": {
		"mo_http.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
		}
	},
	"debug": {
		"trace":true,
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "ses", "$ref": "file://~/private.json#aws_credentials",
				"from_address": "active-data-maintainers@mozilla.com",
				"to_address": "active-data-maintainers@mozilla.com",
				"subject": "[ALERT][DEV]Problem in Push jobs to ES",


			}
		]
	}
}
