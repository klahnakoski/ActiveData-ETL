{
	"constants": {
		"mo_http.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
		},
		"mo_http.big_data.MAX_STRING_SIZE": 100000000
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "unittest",
		"type": "test_result",
		"timeout": 300,
		"schema": {
			"$ref": "//../../schema/test_results.json"
		},
		"debug": true,
		"limit_replicas": false
	},
	"hg": {
		"url":"https://hg.mozilla.org",
		"repo": {
			"host": "http://54.149.35.214",
			"port": 9200,
			"index": "repo",
			"type": "revision",
			"timeout": 300,
			"debug": false,
			"limit_replicas": false
		}
	},
	"debug": {
		"trace": true,
		"cprofile": {
			"enabled": false,
			"filename": "./results/profile.tab"
		},
		"log": [
			{
				"log_type": "console"
			},
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./results/logs/th_dev.log",
				"maxBytes": 10000000,
				"backupCount": 100,
				"encoding": "utf8"
			},
			{
				"log_type": "ses", "$ref": "file://~/private.json#aws_credentials",
				"from_address": "active-data-maintainers@mozilla.com",
				"to_address": "active-data-maintainers@mozilla.com",
				"subject": "[ALERT][Dev] Problem with Treeherder ETL",

			}
		]
	}
}
