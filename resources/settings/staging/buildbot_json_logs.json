{
	"force": false,
	"source": {
		"url": "http://builddata.pub.build.mozilla.org/builddata/buildjson/"
	},
	"destination": {
		"bucket": "active-data-buildbot",
		"public": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"notify": {
		"name": "active-data-etl",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"constants": {
		"mo_http.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
		}
	},
	"debug": {
		"trace": true,
		"cprofile": false,
		"log": [
			{
				"log_type": "console"
			},
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "/logs/buildbot_json_logs.log",
				"maxBytes" : 10000000,
				"backupCount" : 10,
				"encoding" : "utf8"
			},
			{
				"log_type": "ses",
				"from_address": "active-data-maintainers@mozilla.com",
				"to_address": "active-data-maintainers@mozilla.com",
				"subject": "[ALERT][Staging] Problem in Buildbot Scan",
				"$ref": "file://~/private.json#aws_credentials"
			}
		]
	}
}
