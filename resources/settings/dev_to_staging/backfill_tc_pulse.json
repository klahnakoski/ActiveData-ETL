{
	"no_checks": true,
	"range": {"min":2270000, "max":2285517},
	"limit": 1,
	"git": {
		"url": "https://github.com/mozilla/ActiveData-ETL.git",
		"branch": "etl"
	},
	"work_queue": {  //FILL THIS TO TRIGGER ETL ACTION
		"name": "active-data-etl",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"source": {
		"$ref": "file://~/private.json#aws_credentials",
		"bucket": "active-data-task-cluster-logger",
		"prefix":"tc.",
		"key_format":"t.a:b"
	},
	"elasticsearch": {
		"id_field": "etl.source.id",
		"host": "http://activedata.allizom.org",
		"port": 9200,
		"index": "task",
		"type": "task",
		"timeout": 300,
		"debug": true,
		"limit_replicas": true
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "console"
			}
		]
	}
}
