{
	"threads": 2,
	"sample_size": 0.05,
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "unittest",
		"type": "test_result",
		"timeout": 300,
		"schema": {
			"$ref": "//../../schema/test_results.json"
		},
		"debug": false,
		"limit_replicas": false
	},
	"source": {
		"$ref": "file://~/private.json#aws_credentials",
		"bucket": "active-data-test-result-beta",
		"key_format": "a:b.c"
	},
	"work_queue":{
		"name": "active-data-index-unit-beta",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"constants": {
		"mo_http.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
		}
	},
	"debug": {
		"log": [
			{
				"log_type": "console"
			},
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "./results/logs/push_unit_to_es.log",
				"maxBytes" : 10000000,
				"backupCount" : 10,
				"encoding" : "utf8"
			},
			{
				"log_type": "ses", "$ref": "file://~/private.json#aws_credentials",
				"from_address": "active-data-maintainers@mozilla.com",
				"to_address": "active-data-maintainers@mozilla.com",
				"subject": "[ALERT][Beta]Problem in Push to ES",


			}
		]
	}
}
